    body#page-top

        // Masthead
        header
            .container-fluid.py-5.px-0.mb-5
                .col.text-center.text-uppercase
                    h1.font-monospace.strike(style='color:var(--bs-blue)' title="Contact") Contact

       
        // Main
        .container-md
            .row.g-4.gx-lg-5
                // Nav
                #nav.col-lg-2.col-md-12.col.text-uppercase.fw-bold

                   ul.list-group.flex-lg-column.flex-row.list-group-flush.border-dark.border-end.border-2
                        li.list-group-item
                            a.text-decoration-none(href='/' title='') Home
                        li.list-group-item
                            a.text-decoration-none(href='currentwork.html' title='') Current Work
                        li.list-group-item
                            a.text-decoration-none(href='archivelink.html' title='') Archive
                        li.list-group-item
                            a.text-decoration-none(href='about.html' title='') About
                        li.list-group-item.active
                            a.text-decoration-none.text-reset(href='contact.html' title='') Contact

                .col-lg-10.col-sm-12

                    // Contact
                    section#contact.container-md
                            .row.gx-5.align-items-center.text-center
                                .col-lg-6.col-md-6
                                    img.img-fluid(src='assets/img/Handssmall.jpg', alt='...')
                                    p.small.fw-bold
                                        i Hands
                                        | 2015, silver print, 20x20cm.

                                .col-lg-6.col-md-6
                                    p.h4.fw-bold elisavet.kalpaxi@bucks.ac.uk


       
        // Footer
        footer.py-5
            p.strike.font-monospace(style='color:var(--bs-blue)' title="") 
            p.text-center.small.text-muted Copyright &copy; 2023 - Elisavet Kalpaxi

        // Bootstrap core JS
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js')

        // SimpleLightbox plugin JS
        script(src='https://cdnjs.cloudflare.com/ajax/libs/SimpleLightbox/2.1.0/simpleLightbox.min.js')

        // Core theme JS
        script(src='js/scripts.js')
